# Example environment variables for Jamso-Ai-Engine Dashboard
# Copy to .env and fill in real values for production

SECRET_KEY=change_me_in_production
CSRF_SECRET_KEY=change_me_in_production
DASHBOARD_API_KEY=change_me_in_production
SESSION_TYPE=filesystem  # or 'redis' for production
SESSION_FILE_DIR=instance/dashboard_sessions
SESSION_COOKIE_SECURE=False  # Set to True in production with HTTPS
SESSION_COOKIE_HTTPONLY=True
SESSION_COOKIE_SAMESITE=Lax
REDIS_URL=redis://localhost:6379/0  # Uncomment and set if using Redis for session storage
